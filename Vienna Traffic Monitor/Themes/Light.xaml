<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="MainColor" Color="YellowGreen"/>
    <SolidColorBrush x:Key="BackgroundSelected" Color="YellowGreen"/>
    <SolidColorBrush x:Key="BorderBrushSelected" Color="#AED75B"/>
    <SolidColorBrush x:Key="MainColorDark" Color="#6C9023"/>
    <!--Alternative Hintergrundfarbe
    <SolidColorBrush x:Key="LightBackground" Color="#E0F0C0"/>-->
    <SolidColorBrush x:Key="LightBackground" Color="White"/>
    <SolidColorBrush x:Key="BackgroundNormal" Color="White"/>
    <SolidColorBrush x:Key="Foreground" Color="Black"/>
    <SolidColorBrush x:Key="BorderBrush" Color="#797984"/>
    <SolidColorBrush x:Key="BorderBrushNormal" Color="#797984"/>
    <SolidColorBrush x:Key="BackgroundHighlighted" Color="YellowGreen"/>
    <SolidColorBrush x:Key="ButtonBackgroundHighlighted" Color="#AED75B"/>

    <SolidColorBrush x:Key="TextBoxBackground" Color="White"/>
    <SolidColorBrush x:Key="TextBoxBackgroundSelected" Color="#AED75B"/>
    <SolidColorBrush x:Key="SearchTextForeground" Color="Black"/>
    

</ResourceDictionary>